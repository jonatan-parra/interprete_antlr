//Funcion que recibe y retorna valores
SubProceso ans <- doble(ax)
    Definir ans Como Entero;
    ans <- xa*2;
FinSubProceso

//Funcion que no recibe parametros
SubProceso ans <- saludo
    Definir ans Como Caracter;
    ans <- "Hola";
FinSubProceso

//No retorna pero recibe parametros
SubProceso mostrar(xa)
    Escribir xa;
FinSubProceso

SubProceso hola
    Escribir "hola";
FinSubProceso


Algoritmo algo
    mostrar( doble(2) );
    mostrar( saludo );
    mostrar( saludo( a(1,2,3) ) );
    hola; hola                                      ;
    hola;
    saludo                   ( hola                 (a) ) ;
FinAlgoritmo
