

proceso mi_proceso
    definir aa,ba,ca como entero;
    Leer a1;

    segun a1 hacer
        caso ba:
             Escribir "ba";
             ba <- 2;
        caso ba*ca:
             Escribir "ba*ca";
             ca <- 12;
        caso ca:
             Escribir ca;
             ca <- mi_subproceso(2);
             ba <- c*mi_subproceso(3);
             mi_subproceso(ba*ca);
        caso mi_subproceso(2):
             Escribir "6";
        de otro modo:
             Escribir "nada";
    finsegun

finproceso

subproceso x <- mi_subproceso(a)
    x <- a*3;
finfuncion
